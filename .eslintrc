{
  "extends": [
    "eslint-config-airbnb-base",
    "prettier"
  ],
  "plugins": ["prettier"],
  "env": {
    "browser": true,
    "commonjs": true,
    "node": true
  },
  "globals": {
    "$": true,
    "lightGallery": true,
    "magnificPopup": true,
    "Isotope": true,
    "Plyr": true
  },
  "rules": {
    "prettier/prettier": ["error"],
    "global-require": "off",
    "no-param-reassign": "off",
    "prefer-destructuring": "off",
    "prefer-rest-params": "off",
    "no-unused-vars": "off"
  }
}

